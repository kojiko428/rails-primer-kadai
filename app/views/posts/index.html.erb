<h1>投稿一覧</h1>
<p><%= notice %></p>

<table>
  <tr>
      <th>内容</th>
  </tr>

<% @posts.each do |post| %>
   <tr>
   <td><%= post.content %></td>
   <td><%= link_to '確認', post_path(post.id) %></td>
   <td><%= link_to '編集', edit_post_path(post.id) %></td>
   <td><%= link_to '削除', post_path(post.id), method: :delete %></td>
   </tr>
<% end %>
</table>

<%= link_to '新しい投稿をする', new_post_path %>
